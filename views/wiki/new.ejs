            </ul>
          </div>
            <section id="new-post-form">
              <form action="/wiki" method="POST" id="new-form">
                <!-- <label for="wiki[title]">POST Title:</label><br> -->
              <div class="input-field col s12">
                  <input id="title-input-field" type="text" name="wiki[title]"><br>
                  <label for="title">title</label>
              </div>
              <div class="input-field col s12">
                <select name="wiki[category]">
                  <option name="wiki[category.food]">food</option>
                  <option name="wiki[category.tech]">tech</option>
                  <option name="wiki[category.style]">style</option>
                  <option name="wiki[category.travel]">travel</option>
                </select>
              </div>
              <div class="input-field col s12">
                <input type="text" name="wiki[content]"><br>
                <label for="content">content</label>
              </div>
              <!-- <div> -->
                <!-- <form action="/wiki/<= wiki.id %>/like?_method=PUT" method="POST"> -->
                  <input id="new-post-submit-button" class="indigo darken-4 white-text" type="submit" value="add new post">
