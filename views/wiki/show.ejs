<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8">
      <title>Show Page!</title>
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link rel="stylesheet" href="/css/style.css" />
      <script src="/libraries/jquery/jquery.min.js"></script>
      <link rel="stylesheet" href="/libraries/materialize/css/materialize.min.css" />
      <script src="/libraries/materialize/js/materialize.min.js"></script>
      <script src="/js/app.js"></script>
      <script src="libraries/marked.min.js"></script>
  </head>
  <body>
    <nav>
      <div class="nav-wrapper indigo darken-4">
        <ul class="center hide-on-med-and-down">
          <a href="/wiki" class="left hide-on-med-and-down" class="brand-logo">CATEGORY</a>
          <li><a class="btn-floating indigo darken-4" href="/wiki/new"><i class="material-icons">assignment</i></a></li>
          <li><a class="btn-floating indigo darken-4" href="/wiki/tech/show"><i class="material-icons">code</i></a></li>
          <li><a class="btn-floating indigo darken-4" href="/wiki/travel/show"><i class="material-icons">my_location</i></a></li>
          <li><a class="btn-floating indigo darken-4" href="/wiki/style/show"><i class="material-icons">shopping_basket</i></a></li>
          <li><a class="btn-floating indigo darken-4" href="/wiki/food/show"><i class="material-icons">receipt</i></a></li>
        </ul>
        </div>
      </nav>
    <div class="hide-on-small-only">
      <form>
        <div class="input-field">
          <input id="search" type="search" required>
          <label id="search-icon" class="label-icon" for="search"><i class="material-icons">search</i></label>
          <i id="close-icon" class="material-icons">close</i>
        </div>
      </form>
    </div>
  <!-- <div class="col s12 m4 l2">
    <div id="left-column">
          <ul id="dropdown1" class="dropdown-content">
            <li><a id="tech-drop-down" href="/wiki/tech/show">tech</a></li>
            <li class="divider"></li>
            <li><a href="/wiki/travel/show">travel</a></li>
            <li class="divider"></li>
            <li><a href="/wiki/style/show">style</a></li>
            <li class="divider"></li>
            <li><a href="/wiki/food/show">food</a></li>
          </ul>
        <ul class="left hide-on-med-and-down">
          <li><a class="dropdown-button" href="#!" data-activates="dropdown1">CATEGORIES<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
    </div>
  </div> -->

    <div class="col s12 m4 l8">
        <div id="top-categories">
            <div class="fixed-action-btn horizontal click-to-toggle">
              <a class="btn-floating btn-large indigo darken-4">
                <i class="material-icons">menu</i>
              </a>
              <ul>
                <li><a class="btn-floating indigo darken-4" href="/wiki/food/show"><i class="material-icons">receipt</i></a></li>
                <li><a class="btn-floating indigo lighten-3" href="/wiki/style/show"><i class="material-icons">shopping_basket</i></a></li>
                <li><a class="btn-floating indigo darken-4" href="/wiki/travel/show"><i class="material-icons">my_location</i></a></li>
                <li><a class="btn-floating indigo lighten-3" href="/wiki/tech/show"><i class="material-icons">code</i></a></li>
                <li><a class="btn-floating indigo darken-4" href="/wiki/new"><i class="material-icons">assignment</i></a></li>
                <li><a class="btn-floating indigo lighten-3" href="/wiki"><i class="material-icons">view_quilt</i></a></li>
              </ul>
            </div>
        </div>
    </div>

    <div class="col s12 m4 l8">
      <% wiki.forEach(wiki => { %>
        <div id="show-card-panel" class="card-panel indigo lighten-5">
        <div class="card-content">
          <p>Title:<%= wiki.title %></p>
        </div>
        <div class="date-created">
          <% console.log(typeof `${wiki.date_created}`) %>
          <p>Date Created: <%= `${wiki.date_created}`.slice(0,10) %></p>
        </div>
        <!-- <div class="date-created">
          <p>Date Created: <%= wiki.date_created %></p>
        </div> -->
          <p>Category:<%= wiki.category %></p>
          <p> Content:<%= wiki.content %></p>

          <i class="material-icons indigo lighten-3">grade</i>
          <a class="white-text" href="/wiki/<%= wiki.id %>/edit"><i class="material-icons indigo lighten-3">mode_edit</i></a>

        </div>
        <% }) %>
      </div>
    </div>
  </body>
</html>
