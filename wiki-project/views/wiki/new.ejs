        <input type="text" name="wiki[title]" placeholder="Post Title!"><br>
        <input type="text" name="wiki[category]" placeholder="Post Category!"><br>
        <input type="text" name="wiki[content]" placeholder="Insert Content!"><br>
        <!-- <input type="text" name="wiki[date_created]" placeholder="Date Created!"><br> -->
        <!-- <input type="text" name="wiki[date_updated]" placeholder="Date Updated!"><br> -->
        <input type="submit" value="ADD NEW POST">
      </form>
      </section>
    </main>
