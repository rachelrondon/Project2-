<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8">
      <!--Mobile Device Optimization -->
      <!-- <meta name="viewport" content="width-device-width, initial-scale=1.0"/> -->
      <title>Wiki 2.0</title>
      <!-- Materialize CSS Links -->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link rel="stylesheet" href="libraries/materialize/css/materialize.min.css" />
      <!-- App Styles -->
      <link rel="stylesheet" href="/css/style.css" />
      <!-- jQuery -->
      <script src="/libraries/jquery/jquery.min.js"></script>
      <!-- Materialize JS -->
      <script src="/libraries/materialize/js/materialize.min.js"></script>
      <!-- Application JS -->
      <script src="/js/app.js"></script>
  </head>
  <body>
    <div class="show-body">

      <ul id="dropdown1" class="dropdown-content">
        <li><a href="/wiki">launch</a></li>
        <li><a href="/wiki/style/show">style</a></li>
        <li><a href="/wiki/travel/show">travel</a></li>
      </ul>
      <nav>
        <div class="nav-wrapper">
            <a href="#!">Pitch!</a>
          <ul class="right hide-on-med-and-down">
            <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Categories<i class="material-icons right">arrow_drop_down</i></a></li>
          </ul>
        </div>
      </nav>
    <div id="show-nav" class="nav-wrapper">
      <div class="style-heading">
        <!-- <div class="fixed-action-btn click-to-toggle"> -->
          <!-- <a class="btn-floating btn-large red"> -->
            <!-- <i class="material-icons">menu</i> -->
          <!-- </a> -->
      <!-- </div> -->
       </div>
      </nav>
    <div class="row">
    <!-- <div class="container"> -->
      <div class="col s12">
      <div class="show-input">
        <div class="row container">
          <!-- <%= wiki.category %> -->
          <% wiki.forEach(wiki => { %>
            <p class="grey-text text-darken-3 lighten-3">Title:<%= wiki.title %></p>
            <p class="grey-text text-darken-3 lighten-3">Content:<%= wiki.content %></p>
            <p class="grey-text text-darken-3 lighten-3">Date Created:<%= wiki.date_created %></p>
            <% }) %>
          </div>
        </div>
      </div>
    </div>

<!-- Below is the ending div for the row -->
    </div>
  </body>
</html>
