<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8">
      <!--Mobile Device Optimization -->
      <!-- <meta name="viewport" content="width-device-width, initial-scale=1.0"/> -->
      <title>Wiki 2.0</title>
      <!-- Materialize CSS Links -->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link rel="stylesheet" href="libraries/materialize/css/materialize.min.css" />
      <!-- App Styles -->
      <link rel="stylesheet" href="/css/style.css" />
      <!-- jQuery -->
      <script src="/libraries/jquery/jquery.min.js"></script>
      <!-- Materialize JS -->
      <script src="/libraries/materialize/js/materialize.min.js"></script>
      <!-- Application JS -->
      <script src="/js/app.js"></script>
  </head>
  <body>
    <div class="show-body">

      <nav>
        <div class="nav-wrapper">
          <div class="hide-on-small-only">
          <ul id="dropdown1" class="dropdown-content">
            <li><a href="/wiki">launch</a></li>
            <li><a href="/wiki/style/show">style</a></li>
            <li><a href="/wiki/travel/show">travel</a></li>
          </ul>
            <!-- <a href="#!">Pitch!</a> -->
          <ul class="right hide-on-med-and-down">
            <li><a class="dropdown-button" href="#!" data-activates="dropdown1">CATEGORIES<i class="material-icons right">arrow_drop_down</i></a></li>
          </ul>
          </div>
        </div>
      </nav>
      <div class="page-name">Pitch'd: Style</div>
      <div class="style-heading"></div>
    <div class="row">
    <!-- <div class="container"> -->
      <div class="col s12">
        <div class="card">
      <div class="show-input">
        <div class="row container">
           <div class="card-panel">
          <!-- <%= wiki.category %> -->
          <% wiki.forEach(wiki => { %>
            <div class="card-content">
            <span class="white-text">Title:<%= wiki.title %></span>
            </div>
            <div class="card-reveal">
              <!-- <span class="card-title grey-text text-darken-4"> -->
              <span class="card-title grey-text text-darken-4">Content:<%= wiki.content %></span>
              <span class="white-text">Date Created:<%= wiki.date_created %></span>
            <% }) %>
           </div>
          </div>
        </div>
      </div>
    </div>

<!-- Below is the ending div for the row -->
    </div>
  </body>
</html>
